    <validationRules>
        <errorMessage><!-- MTN or Email is required to save the Prospect record. --></errorMessage>
        <name>CCPM_MTNorEmailRequired</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Please check the &apos;Messaging Consent&apos; checkbox. --></errorMessage>
        <name>CCPM_SMSOptIn</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Both the Email&apos;s should match --></errorMessage>
        <name>CCPM_confirmEmail</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Both the MTN&apos;s should match --></errorMessage>
        <name>CCPM_confirmMTN</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Event Type is a mandatory field --></errorMessage>
        <name>CCPM_eventTypeMandatory</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- First Name is a mandatory field --></errorMessage>
        <name>CCPM_firstNameMandatory</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Please enter valid MTN --></errorMessage>
        <name>CCPM_limitMTNto15Numbers</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Zip Code atleast should be five digit --></errorMessage>
        <name>CCPM_limitZipcodeto5</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Event date/time cannot be in Past. Please correct the &apos;SMS Follow Up Date/Time&apos; field. --></errorMessage>
        <name>CCPM_restricChangeToEvent</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Black Friday/Cyber Monday event type is no longer available --></errorMessage>
        <name>CCPM_restrictBlackFriday</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Lead Status must be New --></errorMessage>
        <name>CCPM_restrictLeadStatustoNew</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- First/Last Name Cannot contain special characters or numbers --></errorMessage>
        <name>CCPM_restrictNametoAlphabets</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- Number of Lines Interested In can only contain numbers --></errorMessage>
        <name>CCPM_restrictNoofLinestoNumber</name>
    </validationRules>
    <validationRules>
        <errorMessage><!-- SMS Follow Up Date is a mandatory field to determine when a SMS is sent to the prospect --></errorMessage>
        <name>CCPM_validateEventDateToMarkRequired</name>
    </validationRules>
