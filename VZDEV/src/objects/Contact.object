    <validationRules>
        <fullName>CCPM_retrictNumbersOnContactNames</fullName>
        <active>true</active>
        <description>First/Last Name cannot contain numbers.</description>
        <errorConditionFormula>OR(
NOT(REGEX( FirstName, &quot;[^0-9]{0,255}&quot; )),
NOT(REGEX( LastName, &quot;[^0-9]{0,255}&quot; ))
)</errorConditionFormula>
        <errorMessage>First/Last Name cannot contain numbers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_validMTN</fullName>
        <active>true</active>
        <description>MTN should not be blank and should not contain anything other than numbers</description>
        <errorConditionFormula>OR(
ISNULL(MobilePhone),
NOT(REGEX(MobilePhone, &quot;^[0-9 + (+)+-]*$&quot;))
)</errorConditionFormula>
        <errorMessage>Please enter a valid MTN. It should not contain anything other than numbers.</errorMessage>
    </validationRules>
