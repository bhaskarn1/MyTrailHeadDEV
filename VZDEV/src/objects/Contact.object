      alert(&quot;{!$Label.et4ae5__msg0047}&quot;);
      }
      else if (didResub==&quot;not found&quot;)
      {
      alert(&quot;{!$Label.et4ae5__msg0085}&quot;);
      }
      else if (didResub==&quot;other error&quot;)
