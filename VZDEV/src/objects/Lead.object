        <description>Lead Score Rating Formula : 
If lead score is below 0.99 THEN icon: /img/samples/stars_000.gif
If lead score is between 1 and 2.99 THEN icon: /img/samples/stars_100.gif 
If lead score is between 3 and 4.99 THEN icon: /img/samples/stars_200.gif
If lead score is between 5 and 6.99 THEN icon: /img/samples/stars_300.gif
If lead score is between 7 and 8.99 THEN icon: /img/samples/stars_400.gif
If lead score is greater than 9 THEN icon: /img/samples/stars_500.gif</description>
        <externalId>false</externalId>
        <formula>IF( CCPM_customerAndLeadCombinedScoreFormula__c &lt; 1, 
IMAGE(&quot;/img/samples/stars_000.gif&quot;, &quot;Lead Score&quot;),
IF( (1&lt;= CCPM_customerAndLeadCombinedScoreFormula__c) &amp;&amp; (CCPM_customerAndLeadCombinedScoreFormula__c &lt; 3),
IMAGE(&quot;/img/samples/stars_100.gif&quot;, &quot;Lead Score&quot;),
IF( (3&lt;= CCPM_customerAndLeadCombinedScoreFormula__c) &amp;&amp; (CCPM_customerAndLeadCombinedScoreFormula__c &lt; 5),
IMAGE(&quot;/img/samples/stars_200.gif&quot;, &quot;Lead Score&quot;),
IF( (5&lt;= CCPM_customerAndLeadCombinedScoreFormula__c) &amp;&amp; (CCPM_customerAndLeadCombinedScoreFormula__c &lt; 7),
IMAGE(&quot;/img/samples/stars_300.gif&quot;, &quot;Lead Score&quot;),
IF( (7&lt;= CCPM_customerAndLeadCombinedScoreFormula__c) &amp;&amp; (CCPM_customerAndLeadCombinedScoreFormula__c &lt; 9),
IMAGE(&quot;/img/samples/stars_400.gif&quot;, &quot;Lead Score&quot;),
IF( (CCPM_customerAndLeadCombinedScoreFormula__c &gt;= 9),
IMAGE(&quot;/img/samples/stars_500.gif&quot;, &quot;Lead Score&quot;),
null
))))))</formula>
