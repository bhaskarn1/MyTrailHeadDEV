    <validationRules>
        <fullName>CCPM_MTNorEmailRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$RecordType.Name == &apos;Prospect&apos;,

AND(

ISBLANK(CCPM_mobileNumber__c),
ISBLANK(CCPM_email__c)
)
)</errorConditionFormula>
        <errorMessage>MTN or Email is required to save the Prospect record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_SMSOptIn</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR( 
$RecordType.Name == &apos;Prospect&apos;, 
AND(
$RecordType.Name == &apos;Customer Cart&apos;,
CCPM_Type__c==&apos;Manual Lead&apos;
)
),
CCPM_SMSLegalOptIn__c == False 
)</errorConditionFormula>
        <errorMessage>Please check the &apos;Messaging Consent&apos; checkbox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_confirmEmail</fullName>
        <active>true</active>
        <description>Email and confirm Email should match</description>
        <errorConditionFormula>AND(
$RecordType.Name == &apos;Prospect&apos;,

(CCPM_email__c) &lt;&gt; (CCPM_confirmEmail__c),

AND( $Profile.Name &lt;&gt; &apos;System Administrator&apos;, $Profile.Name &lt;&gt; &apos;CCPM Integration User&apos;)
)</errorConditionFormula>
        <errorDisplayField>CCPM_confirmEmail__c</errorDisplayField>
        <errorMessage>Both the Email&apos;s should match</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_confirmMTN</fullName>
        <active>true</active>
        <description>MTN and confirm MTN should match</description>
        <errorConditionFormula>AND(
$RecordType.Name == &apos;Prospect&apos;,

(CCPM_mobileNumber__c) &lt;&gt; (CCPM_confirmMTN__c),

AND( $Profile.Name &lt;&gt; &apos;System Administrator&apos;, $Profile.Name &lt;&gt; &apos;CCPM Integration User&apos;)
)</errorConditionFormula>
        <errorDisplayField>CCPM_confirmMTN__c</errorDisplayField>
        <errorMessage>Both the MTN&apos;s should match</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_eventTypeMandatory</fullName>
        <active>true</active>
        <description>Check if Event Type field is filled out</description>
        <errorConditionFormula>AND(
$RecordType.Name == &apos;Prospect&apos;, 

ISBLANK(TEXT(CCPM_eventType__c)),

AND( $Profile.Name &lt;&gt; &apos;System Administrator&apos;, $Profile.Name &lt;&gt; &apos;CCPM Integration User&apos;)
)</errorConditionFormula>
        <errorDisplayField>CCPM_eventType__c</errorDisplayField>
        <errorMessage>Event Type is a mandatory field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_firstNameMandatory</fullName>
        <active>true</active>
        <description>Check if First Name is filled out</description>
        <errorConditionFormula>AND(
$RecordType.Name == &apos;Prospect&apos;,

ISBLANK(FirstName),


AND( $Profile.Name &lt;&gt; &apos;System Administrator&apos;, $Profile.Name &lt;&gt; &apos;CCPM Integration User&apos;)
)</errorConditionFormula>
        <errorDisplayField>FirstName</errorDisplayField>
        <errorMessage>First Name is a mandatory field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_limitMTNto15Numbers</fullName>
        <active>true</active>
        <description>MTN should be limited to 10 characters to prevent adding numbers</description>
        <errorConditionFormula>AND(
$RecordType.Name == &apos;Prospect&apos;,

AND(
      NOT(ISNULL( CCPM_mobileNumber__c )),
      OR(

          LEN(CCPM_mobileNumber__c) &gt; 15,
          NOT( REGEX( CCPM_mobileNumber__c , &quot;^[0-9 + (+)+-]*$&quot;))
    )
)
)</errorConditionFormula>
        <errorDisplayField>CCPM_mobileNumber__c</errorDisplayField>
        <errorMessage>Please enter valid MTN</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_limitZipcodeto5</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$RecordType.Name == &apos;Prospect&apos;,

AND
( 
NOT(ISBLANK(PostalCode)),
NOT(REGEX(PostalCode, &quot;\\d{5}?&quot;))
)
)</errorConditionFormula>
        <errorMessage>Zip Code atleast should be five digit</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_restricChangeToEvent</fullName>
        <active>true</active>
        <description>Do not allow update to existing event and event date.</description>
        <errorConditionFormula>AND (OR( AND(NOT(ISNEW()),OR(ISCHANGED( CCPM_eventType__c ) , ISCHANGED( CCPM_SMSFollowUpDate__c )),(OR(PRIORVALUE(CCPM_SMSFollowUpDate__c ) &lt;= NOW(),CCPM_SMSFollowUpDate__c &lt;= NOW()))), AND(ISNEW(),(CCPM_SMSFollowUpDate__c &lt;= NOW()))),AND( $Profile.Name &lt;&gt; &apos;System Administrator&apos;, $Profile.Name &lt;&gt; &apos;CCPM Integration User&apos;) )</errorConditionFormula>
        <errorMessage>Event date/time cannot be in Past. Please correct the &apos;SMS Follow Up Date/Time&apos; field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_restrictBlackFriday</fullName>
        <active>false</active>
        <description>Black Friday/Cyber Monday Event is no longer available</description>
        <errorConditionFormula>AND(
OR(RecordType.Name == &apos;Prospect&apos;, AND(RecordType.Name == &apos;Customer Cart&apos;, CCPM_Type__c == &apos;Manual Lead&apos;)),
AND(ISPICKVAL(CCPM_eventType__c,&apos;Black Friday/Cyber Monday&apos;), OR(ISNEW(), (ISCHANGED(CCPM_eventType__c)))), 
AND( $Profile.Name &lt;&gt; &apos;System Administrator&apos;, $Profile.Name &lt;&gt; &apos;CCPM Integration User&apos;)
)</errorConditionFormula>
        <errorMessage>Black Friday/Cyber Monday event type is no longer available</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_restrictLeadStatustoNew</fullName>
        <active>true</active>
        <description>Lead Status Must be new while creating the prospect</description>
        <errorConditionFormula>AND(
ISNEW(),
RecordType.Name == &apos;Prospect&apos;,
OR(  AND ( ISPICKVAL(Status,&apos;Eligible&apos;),
           OR ( CCPM_previousLeadStatus__c  &lt;&gt; &apos;New&apos;,CCPM_mobileNumber__c = &apos;&apos; )),
     AND ( ISPICKVAL(Status,&apos;Contacted&apos;), CCPM_previousLeadStatus__c  &lt;&gt; &apos;Eligible&apos;),
     ISPICKVAL( Status , &apos;Responded&apos;),
     ISPICKVAL( Status , &apos;Qualified&apos;),
     ISPICKVAL( Status , &apos;Converted&apos;),
     ISPICKVAL( Status , &apos;Expired&apos;),
     ISPICKVAL( Status , &apos;Dismissed&apos;)
), 
AND( $Profile.Name &lt;&gt; &apos;System Administrator&apos;, $Profile.Name &lt;&gt; &apos;CCPM Integration User&apos;) 
)</errorConditionFormula>
        <errorMessage>Lead Status must be New</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_restrictNametoAlphabets</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$RecordType.Name == &apos;Prospect&apos;,

OR(
   AND(NOT(ISBLANK(FirstName)), NOT( REGEX( FirstName , &quot;^[a-z + A-Z]*$&quot;))),
   NOT( REGEX( LastName , &quot;^[a-z + A-Z]*$&quot;))
)
)</errorConditionFormula>
        <errorMessage>First/Last Name Cannot contain special characters or numbers</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_restrictNoofLinestoNumber</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$RecordType.Name == &apos;Prospect&apos;,

OR(
   NOT( REGEX( Company , &quot;^[0-9]*$&quot;)))
)</errorConditionFormula>
        <errorMessage>Number of Lines Interested In can only contain numbers</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_validateEventDateToMarkRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
$RecordType.Name == &apos;Prospect&apos;,

ISBLANK( CCPM_SMSFollowUpDate__c),
AND( $Profile.Name &lt;&gt; &apos;System Administrator&apos;, $Profile.Name &lt;&gt; &apos;CCPM Integration User&apos;)
)</errorConditionFormula>
        <errorDisplayField>CCPM_SMSFollowUpDate__c</errorDisplayField>
        <errorMessage>SMS Follow Up Date is a mandatory field to determine when a SMS is sent to the prospect</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_validateLeadStatusChangePrivilege</fullName>
        <active>true</active>
        <description>If the user is not System Administrator or CCPM Integration User he/she cannot change the Lead Status to New, Eligible, Contacted, Responded, Converted and Expired</description>
        <errorConditionFormula>AND( ISCHANGED(Status), 
      OR( ISPICKVAL(Status,&apos;New&apos;), 
          ISPICKVAL(Status,&apos;Eligible&apos;),
          ISPICKVAL(Status,&apos;Contacted&apos;),
          ISPICKVAL(Status,&apos;Responded&apos;), 
          ISPICKVAL(Status,&apos;Converted&apos;), 
          ISPICKVAL(Status,&apos;Expired&apos;), 
          ISPICKVAL(Status,&apos;Dismissed&apos;)
         ),
         AND( $Profile.Name &lt;&gt; &apos;System Administrator&apos;, $Profile.Name &lt;&gt; &apos;CCPM Integration User&apos;)
      )</errorConditionFormula>
        <errorMessage>You are not allowed to change the Lead Status to New, Eligible, Contacted, Responded, Converted and Expired</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_validateLeadSubStatusChgPrivilege</fullName>
        <active>true</active>
        <description>Validation rule to ensure that Lead Sub status cannot be changed if the Lead Status is &apos;Responded&apos;</description>
        <errorConditionFormula>AND(
   ISCHANGED(CCPM_leadSubStatus__c),
   ISPICKVAL(Status,&apos;Responded&apos;),
   OR(
   ISPICKVAL(CCPM_leadSubStatus__c,&apos;&apos;),
   ISPICKVAL(CCPM_leadSubStatus__c,&apos;Opted Out&apos;),
   ISPICKVAL(CCPM_leadSubStatus__c,&apos;Action Required&apos;)),
   
   AND(
   $Profile.Name &lt;&gt; &apos;System Administrator&apos;,
   $Profile.Name &lt;&gt; &apos;CCPM Integration User&apos;)
)</errorConditionFormula>
        <errorMessage>You cannot change the Lead Sub-Status when the Lead Status is Responded. You can only change the Lead Status to Qualified and then change the associated Sub-Status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CCPM_validateLeadUpdatePriviledge</fullName>
