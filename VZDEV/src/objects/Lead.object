    <fields>
        <fullName>CCPM_Created_In_vs_Converted_In_Channel__c</fullName>
        <description>Formula field for report grouping based on Lead source and closed lead channel.</description>
        <externalId>false</externalId>
        <formula>CASE( CCPM_leadChannel__c , 
     &quot;Digital&quot;, 
     CASE( CCPM_closedLeadChannel__c , 
          &quot;Digital&quot;,&quot;Created = Digital &amp; Converted = Digital&quot;,
          &quot;Retail&quot;,&quot;Created = Digital &amp; Converted = Retail&quot;,
          &quot;&quot;
          ),
     &quot;Retail&quot;,
     CASE( CCPM_closedLeadChannel__c , 
          &quot;Digital&quot;,&quot;Created = Retail &amp; Converted = Digital&quot;,
          &quot;Retail&quot;,&quot;Created = Retail &amp; Converted = Retail&quot;,
          &quot;&quot;
          ),
     &quot;&quot;
     )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Created In vs Converted In Channel</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
